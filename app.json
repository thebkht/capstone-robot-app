{
  "expo": {
    "name": "Rovy Controller",
    "slug": "Rovy Controller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rovycontroller",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rovy.rovycontroller",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "Rovy Controller uses Bluetooth to securely provision and connect to your Rovy.",
        "icon": {
          "dark": "./assets/images/icons/ios-dark.png",
          "light": "./assets/images/icons/ios-light.png",
          "tinted": "./assets/images/icons/ios-tinted.png"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#352CF4",
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "monochromeImage": "./assets/images/icons/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "minSdkVersion": 23,
      "package": "com.thefalsenet.Rovycontroller"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#161616"
        }
      ],
      "expo-secure-store",
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": ["peripheral", "central"],
          "bluetoothAlwaysPermission": "Allow Rovy Controller to connect to bluetooth devices"
        }
      ],
      "./plugins/withAndroidBlePermissions",
      "./plugins/withAndroidBuildGradleTweaks",
      "./plugins/withPodfileBleCleanup"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "84365424-b8dc-4997-ad48-d883e582aadf"
      }
    }
  }
}
